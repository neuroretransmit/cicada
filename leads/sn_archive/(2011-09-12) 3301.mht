function readFavorities() {
	var data =3D getStored('DESU_Favorities');
	var arr =3D data ? data.split('|') : [];
	for(var i =3D 0; i < arr.length/4; i++)
		Favor[i] =3D [arr[i*4], arr[i*4 + 1], arr[i*4 + 2], arr[i*4 + 3]].join('|=
');
}

function removeFavorities(key) {
	for(var i =3D 0; i < Favor.length; i++)
		if(Favor[i].indexOf(key) > -1) {
			var post =3D pByNum[Favor[i].split('|')[2]];
			if(post) $x(
				'.//a[@class=3D"DESU_icn_favset" or @class=3D"DESU_icn_favor"]'
				, post.Btns).className =3D 'DESU_icn_favor';
			Favor.splice(i, 1);
		}
}

function getFavorKey(post) {
	return host + '|' + brd
		+ (/\/arch/.test(window.location.pathname) ? '/arch' : '') + '|' + post.N=
um;
}

function storeFavorities(post, btn) {
	readFavorities();
	var key =3D getFavorKey(post);
	if(Favor.join('|').indexOf(key) > -1) {
		removeFavorities(key);
		btn.className =3D 'DESU_icn_favor';
	} else {
		Favor[Favor.length] =3D key + '|'
			+ getTitle(post).replace(/\|/g, '').substring(0, !sav.cookie ? 70 : 25);
		Favor.sort();
		if(sav.cookie && encodeURIComponent(Favor.join('%7C')).length > 4095)
			{$alert(Lng.cookiesLimit);Favor.pop();return}
		btn.className =3D 'DESU_icn_favset';
	}
	setStored('DESU_Favorities', Favor.join('|'));
	var div =3D $id('DESU_favor');
	if(div.hasChildNodes()) {$delCh(div);favorThrdsPreview()}
}

