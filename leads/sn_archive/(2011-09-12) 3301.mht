function htmlReplace(txt) {
	if(ch._4ch || ch.krau)
		txt =3D txt.replace(/(^|>|\s)(https*:\/\/.*?)($|<|\s)/ig, '$1<a href=3D"$=
2">$2</a>$3');
	var exp =3D getStored('DESU_RegExpr');
	if(Cfg[13] =3D=3D 0 || !exp || !/\$rep /.test(exp)) return txt;
	exp =3D exp.split('\n');
	var i =3D exp.length;
	while(i--) {
		var x =3D exp[i];
		if(/\$rep /.test(x)) {
			var re =3D x.match(/\/.*[^\\]\/[ig]*/)[0];
			var l =3D re.lastIndexOf('/');
			var wrd =3D x.substr(x.indexOf(re) + re.length + 1);
			re =3D new RegExp(re.substr(1, l - 1), re.substr(l + 1));
			txt =3D txt.replace(re, wrd);
		}
	}
	return txt;
}

function wrongRegExp(txt) {
	txt =3D txt.split('\n');
	var i =3D txt.length;
	while(i--)
		if(/\$exp |\$rep /.test(txt[i])) try {
			x =3D txt[i].match(/\/.*[^\\]\/[ig]*/)[0];
			var l =3D x.lastIndexOf('/');
			new RegExp(x.substr(1, l - 1), x.substr(l + 1));
		} catch(e) {return txt[i]}
	return null;
}

