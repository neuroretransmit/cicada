function getEventX(event) {
	var posx =3D 0;
	if (event.pageX || event.pageY) {
		posx =3D  event.pageX;
	}
	else if (event.clientX || event.clientY) 	{
		posx =3D event.clientX + document.body.scrollLeft
			+ document.documentElement.scrollLeft;
	}
	return posx;
}
function getElementX(obj) {
	var x =3D 0;
	if (obj.offsetParent) {
		do {
			x +=3D obj.offsetLeft;
		} while (obj =3D obj.offsetParent);
	}
	return x;
}
function zeroPad(str,len) {
	var i;
	var pad =3D "";
	var s =3D str.toString();
	for(i=3Ds.length; i < len; i++) {
		pad =3D "0".toString() + pad.toString();
	}
	return pad.toString() + s.toString();
}

function dateToTimestamp(date) {
	return date.getFullYear() +
		zeroPad(date.getMonth()+1,2) +
		zeroPad(date.getDay()+1,2) +
		zeroPad(date.getHours(),2) +
		zeroPad(date.getMinutes(),2) +
		zeroPad(date.getSeconds(),2);
}

function calcTimestamp(event,element,firstMS,lastMS) {
	  var eventX =3D getEventX(event);
	  var elementX =3D getElementX(element);
	  var elementWidth =3D element.width;
	  var msWidth =3D lastMS - firstMS;
	  var x =3D eventX - elementX;
	  var pct =3D x / elementWidth;
	  var pctDate =3D pct * msWidth;
	  var date =3D pctDate + firstMS;
	  return dateToTimestamp(new Date(date));
}

