function ajaxRefmap(txt, pNum)
{
	var x =3D txt.match(/&gt; &gt; \d+/g);

	if(x) for(var i =3D 0; rLen =3D x.length, i < rLen; i++)
			getRefMap(pNum, x[i].match(/\d+/g));
}

function showRefMap(post, pNum, isUpd)
{
	if(typeof refMap[pNum] !=3D=3D 'object' || !post) return;

	var txt =3D '=D0=9E=D1=82=D0=B2=D0=B5=D1=82=D1=8B: ' + refMap[pNum].toStri=
ng().replace(/(\d+)/g, '<a href=3D"#$1" onmouseover=3D"showPostPreview(even=
t)" onmouseout=3D"delPostPreview(event)">&gt;&gt;$1</a>');
	var el =3D isUpd ? $id('ABU_refmap_' + pNum) : null;

	if(!el)
	{
		el =3D $new('div', {'class': 'ABU_refmap', 'id': 'ABU_refmap_' + pNum, 'h=
tml': txt});
		$after($t('blockquote', post)[0], [el]);

	}
	else $html(el, txt);
}

function addRefMap(post)
{
	$each($t('a', post), function(link)
	{
		if(link.textContent.indexOf('>>') =3D=3D 0)
		{
			var rNum =3D link.hash.match(/\d+/);

			if(postByNum[rNum])
			{
				while((link =3D link.parentNode).tagName !=3D 'BLOCKQUOTE');

				getRefMap(link.parentNode.id.match(/\d+/), rNum);
			}
		}
	});

	for(var rNum in refMap)
		showRefMap(postByNum[rNum], rNum, Boolean(post));
}

