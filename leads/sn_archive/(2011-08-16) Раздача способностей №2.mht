function storeHiddenThread(id)
{
	id =3D 't' + id;
	var data =3D get_cookie(thread_cookie);

	if(data.indexOf(id + ',') !=3D -1) return;

	else set_cookie(thread_cookie, data + id + ',', 365);
}

