function getRefMap(pNum, rNum)
{
	if(!refMap[rNum]) refMap[rNum] =3D [];

	if((',' + refMap[rNum].toString() + ',').indexOf(',' + pNum + ',') < 0)
		refMap[rNum].push(pNum);
