function applyRegExp(txt) {
	var fld =3D $id('DESU_regexp');
	var val =3D fld.value;
	if(txt) {
		if(txt.trim() =3D=3D '') return;
		toggleRegexp();
		var nval =3D '\n' + val;
		var ntxt =3D '\n' + txt;
		val =3D nval.indexOf(ntxt) > -1 ? nval.split(ntxt).join('') : val + ntxt;
	}
	val =3D val.replace(/[\r\n]+/g, '\n').replace(/^\n|\n$/g, '');
	var wrong =3D wrongRegExp(val);
	if(wrong) {$alert(Lng.error + ' ' + wrong);return}
	fld.value =3D val;
	forAll(function(post) {if(doRegexp(post)) unhidePost(post)})
	setStored('DESU_RegExpr', val);
	$id('DESU_regexp_ch').checked =3D val !=3D '';
	if(val !=3D '') {
		saveCfg(13, 1);
		forAll(hideByRegexp);
		storeHiddenPosts();
