		}
		for(var j =3D 0; j < word.length; j++) {
			if(up.charAt(j) =3D=3D lw.charAt(j)) continue;
			if(word.charAt(j) =3D=3D up.charAt(j)) upc++;
			else if(word.charAt(j) =3D=3D lw.charAt(j)) lwc++;
		}
		var min =3D upc < lwc ? upc : lwc;
		if(min >=3D 2 && lwc + upc >=3D 5) n++;
	}
	if(n/all >=3D 0.3 && all > 8) return 'cAsE words: ' + parseInt(n/len*100) =
+ '%';
