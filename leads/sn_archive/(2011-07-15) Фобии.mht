function getTitle(post)
{
	var t =3D $c('filetitle', post)[0];

	if(t) t =3D t.textContent.trim();

	if(!t || t =3D=3D '') t =3D $t('blockquote', post)[0].textContent;

	return t.replace(/\s+/g, ' ');
}

function storeHiddenThread(id)
{
	id =3D 't' + id;
	var data =3D get_cookie(thread_cookie);

	if(data.indexOf(id + ',') !=3D -1) return;

	else set_cookie(thread_cookie, data + id + ',', 365);
}

function removeHiddenThread(id)
{
	id =3D 't' + id;
	var data =3D get_cookie(thread_cookie);
	var re =3D new RegExp(id + ',', 'g');
	data =3D data.replace(re, '');
	set_cookie(thread_cookie, data, 365);
}

function toggleHidden(id)
{
	var tNum =3D id.match(/\d+/);
	var thrd =3D $id('thread_' + tNum);
	hideThread(thrd, tNum);
}

function toggleThread(pNum)
{
	hideThread(postByNum[pNum].parentNode, pNum);
	storeHiddenThread(pNum)
}

function unhideThread(thrd, tNum)
{
	$disp(thrd);
	$del($id('ABU_hiddenthr_' + tNum));
	removeHiddenThread(tNum);
}

function hideThread(thrd, tNum)
{
