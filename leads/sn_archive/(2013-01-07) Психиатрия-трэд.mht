// CAPTCHA FUNCS

function reverseCaptcha(e)
{
	if(e.which < 1040 || e.which > 1279)
	{
		return;
	}
	e.preventDefault();

	// russian alphabet in english layout
	var data =3D "f,dult;pbqrkvyjghcnea[wxio]sm'.z";
	// 1072 is equals to russian a
	var index =3D e.which - 1072;
	if(index < 0 || index >=3D data.length)
	{
		return;
	}
	var key =3D data[index];
	// only alpha characters
	if (key < 'a' || key > 'z')
	{
		return;
	}

	e.target.value =3D e.target.value + key;
}

